{
  "name": "content-service",
  "version": "1.0.0",
  "description": "Content Management Service with Rich Text Editor for QR Generation SaaS",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "typecheck": "tsc --noEmit",
    "migrate:static": "ts-node scripts/migrate-static-content.ts",
    "migrate:static:help": "echo 'Usage: npm run migrate:static -- --source-path=\"/path/to/frontend/src\" [--dry-run|--execute] [--overwrite]'"
  },
  "keywords": [
    "content-management",
    "cms",
    "rich-text-editor",
    "quill",
    "blog",
    "testimonials",
    "seo"
  ],
  "dependencies": {
    "aws-sdk": "^2.1502.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "highlight.js": "^11.9.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "markdown-it": "^13.0.2",
    "marked": "^9.1.6",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "quill": "^1.3.7",
    "quill-delta": "^5.1.0",
    "quill-delta-to-html": "^0.12.1",
    "robots-parser": "^3.0.1",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.33.1",
    "sitemap": "^7.1.1",
    "slugify": "^1.6.6",
    "turndown": "^7.1.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/markdown-it": "^13.0.7",
    "@types/marked": "^5.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/sanitize-html": "^2.9.4",
    "@types/turndown": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
